  <!--storageConnectionString="UseDevelopmentStorage=true;DevelopmentStorageProxyUri=http://ipv4.fiddler" -->
  <!--storageConnectionString" value="DefaultEndpointsProtocol=https;AccountName=[AccountName];AccountKey=[AccountKey]" -->

  <appSettings>
    <!--Deprecated ElCamino.AspNet.Identity.AzureTable settings-->
    <!--<add key="StorageConnectionString" value="DefaultEndpointsProtocol=https;AccountName=[AccountName];AccountKey=[AccountKey]" /> -->
    <add key="StorageConnectionString" value="UseDevelopmentStorage=true;" />
  </appSettings>

  <runtime>

